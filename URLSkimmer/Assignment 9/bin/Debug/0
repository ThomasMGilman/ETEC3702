        searchBox.includePastCheckBox = 'SideBar_ctl00_cbIncludePast';
        searchBox.showHideLabel = 'SideBar_ctl00_lblShowHide';
        searchBox.userId = "b0b00000-c542-4d2a-86e6-55ebbbcd85da";
        searchBox.autoCompleteSelect = function(event, ui) {
            $("#" + searchBox.textBox).val(ui.item.value);
            __doPostBack('SideBar$ctl00$lbSearch','');
        };
        searchBox.initAutoComplete();
        $('#SideBar_ctl00_lbSearch').keypress(function(e) {
            var key = e.which;
            if (key === 32 || key === 13) {
                if (searchBox.searchClick()) {
                    window.location.href = this.href;
                }
                return false;
            }
        });   
    });
