    jQuery(document).ready(function ($) {
        searchBox.textBox = 'SideBar_ctl00_tbSearch';
        searchBox.button = 'SideBar_ctl00_lbSearch';
        searchBox.showInSideBar = 'True';
        searchBox.whereDropDown = 'SideBar_ctl00_ddlWhere';
